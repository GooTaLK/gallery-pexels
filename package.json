{
  "name": "gallery-pexels-api",
  "version": "0.0.0",
  "author": "gootalk",
  "description": "This project consists of making use of the Pexels API. It is an image gallery made with React.",
  "keywords": [
    "gallery",
    "react",
    "pexels-api"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "pexels": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "eslint-plugin-react": "^7.28.0",
    "standard": "^16.0.4",
    "vite": "^2.7.2"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    },
    "ignorePatterns": [
      "/dist/**/*.js"
    ]
  }
}
